/* 主页图片 */
.focus-page{
	width: 100%;
    background-color: #000;
    position: relative;
}
.focus-page .focus-img-wrap{
	height:520px;
	position:relative;

}
.focus-page .focus-img-wrap li{
	position: absolute;
	height:100%;
	width: 100%;
	opacity: 0;
	transition: opacity 1500ms;
}

ul,li{
	list-style:none;
	margin:0;
	padding:0;
}
a{
	text-decoration:none;
}
.focus-page .img-item{
	height:100%;
	background:no-repeat 50%;
	background-size:100% 100%;
}
.focus-page .img-link{
	display:block;
	width:100%;
}
.img-list img{
	width:100%;
	height:100%;
}
.img-item1 li{
	position: absolute;
	height:100%;
	width: 100%;
	opacity: 0;
	transition: opacity 1500ms;
}

/* 分类标签 */
.focus-type{
	width:100%;
	height:120px;
	overflow:hidden;
	background:#f9f9f9;
	margin:10px 0 10px 0;
}
.focus-type .type-inner{
	width:920px;
	height:92px;
	font-size:0;
	margin:10px auto;
}
.focus-type .type-wrap{
	height:100%;
	width:100%;
	overflow:hidden;
	font-size:0;
}
.focus-type .type-part{
	display: inline-block;
    color: #222;
    height: 100%;
    overflow: hidden;
}
.focus-type .type-left
{
	width:24%;
}
.focus-type .type-mid
{
	width:33%;
}
.focus-type .type-right
{
	width:43%;
}
h4{
	font-weight:400px;
	margin:0;
	padding:0;
}
.focus-type .type-title{
	font-size:16px;
	line-height:32px;
	color:#999;
}
.focus-type .type-title .t-link{
	font-size:14px;
	margin-left:13px;
	color:#222;
	font-weight: normal;
}
.focus-type .type-list{
	margin-left:-20px;
	margin-right:50px;
}
.focus-type .type-li{
	margin-left:20px;
	display:inline-block;
	font-size:14px;
}
.focus-type .type-link{
	line-height:30px;
	color:#222;
}

/* 电影播放 */
.video-content{
	width: 1285px;
	height:390px;
	position: relative;
    margin: 0 auto;
    overflow: hidden;
    padding: 0 4px;
	margin-bottom:30px;
}
.video-content .video-header{
	position: relative;
	margin-bottom: 15px;
	height:28px;
}
.video-content .video-header-title{
	color: #222;
    font-size: 0;
}
.video-content .header-word{
	float: left;
	font-size: 26px;
    line-height: 28px;
}
.video-wrap-left{
	float:left;
	margin-right:5px;
	width: 640px;
    height: 339px;
	display: inline-block;
    vertical-align: top;
    position: relative;
}
.video-content .video-box{
	width:640px;
	height:339px;
}
.video-box video{
	width:100%;
	height:100%;
}
.video-wrap-right{
	float:right;
	width: 640px;
    height: 339px;
	display: inline-block;
    vertical-align: top;
    position: relative;
}
.content img{   /* 鼠标上去会变亮*/
  opacity:1;
  filter(alpha=100);
}
.content img:hover{
  opacity:0.7;
  filter(alpha=70);
}
.div1{
	width:640px;
	height:339px;
}
.div1 .div1-ul{
	font-size:0;
	white-space:nowrap;
	height:339px;
}
.div1 .div1-li{
	vertical-align: top;
    display: inline-block;
	height:339px;
}
.div1-wrap-li{
	width: 210px;
}
.div1-wrap-li .div1-link{
	width:210px;
	height:280px;
	display:block;
	overflow:hidden;
}
.div1-wrap-li .div1-img{
	display:block;
    height:100%;
	width:100%;
    border-radius:10px;
}
.div1-wrap-li .div1-title-wrap{
	position: relative;
    background: #f8f8f8;
    line-height: 1;
    padding: 6px 10px;
	height:47px;
}
.div1-wrap-li .div1-title-wrap .div1-p-main{
	line-height: 23px;
    height: 23px;
    font-size: 15px;
    vertical-align: baseline;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.div1-wrap-li .div1-title-wrap .div1-link-txt{
	color:#222;
}

